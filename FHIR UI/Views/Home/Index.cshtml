@model FHIR_UI.Models.ResourceTypesModel
@{
    ViewData["Title"] = "Home Page";
}

<html>
<head>
    <title>Заголовок</title>
</head>
<body>
    <div class="panel">
        <div class=" = panel-heading">
            <h3> Просмотр списка ресурсов </h3>
        </div>

        <div class="panel-body">
            <left>
                <a href="FhirResource/Create" class=" btn btn-link" > Создать новый ресурс</a>
            </left>

            <center>
                <table width="1000px">
                    <tr height="500px">
                        <td bgcolor="#EFEEFF" width="20px" valign="top">
                            @foreach (var v in Model.TypesAsArray_)
                            {
                                <p> <a href="Search?type=@v.ToString()"> *@v.ToString() </a> </p>
                            }
                        </td>
                        <td bgcolor="#F8FEFE" valign="top">
                        </td>
                    </tr>
                </table>
            </center>

        </div>
        <div class="panel-footer">
            <p>:)</p>
        </div>

    </div>
</body>
</html>