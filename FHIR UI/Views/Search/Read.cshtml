﻿@using FHIR_UI.Models
@model FHIR_UI.Models.JsonTextModel
@{
    ViewData["Title"] = "Json";
}
<h2>JSON file</h2>
<body>
    @{
        if (Model.JsonText_ != null)
        {
             <p> @Model.JsonText_ </p>
        }
        else
        {
             <p> JSON file is empty </p>
        }
    }
</body>